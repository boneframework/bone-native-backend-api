{
  "name": "delboy1978uk/bone-native-backend-api",
  "description": "A pre-configured Bone Framefork OAuth2 API for use with Bone Native",
  "authors": [
    {
      "name": "Derek Stephen McLean",
      "email": "delboy1978uk@gmail.com",
      "role": "developer"
    }
  ],
  "keywords": [
    "bone",
    "framework",
    "api",
    "native",
    "ios",
    "android",
    "oauth2",
  ],
  "homepage": "https://github.com/delboy1978uk/bone-native-backend-api",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "delboy1978uk/bone": "^3.9",
    "delboy1978uk/cdn": "^2.0",
    "delboy1978uk/image": "^2.1",
    "delboy1978uk/bone-mail": "^1.1",
    "delboy1978uk/bone-doctrine": "^1.6.0",
    "delboy1978uk/bone-paseto": "^1.0",
    "delboy1978uk/bone-user": "^2.2",
    "delboy1978uk/bone-oauth2": "^1.5",
    "delboy1978uk/bone-open-api": "^1.2",
    "delboy1978uk/bone-user-api": "^1.0",
    "delboy1978uk/bone-settings": "^1.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "codeception/codeception": "^4.0",
    "squizlabs/php_codesniffer": "^3.4",
    "codeception/module-asserts": "^1.1",
    "delboy1978uk/bone-vendor-dev": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Bone\\App\\": "src/App/",
      "Bone\\Test\\": "tests/unit/",
      "Fixtures\\": "data/fixtures/"
    },
    "classmap": ["data/proxies/"]
  }
}
